{
  "name": "node-express-decorators-boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "rimraf dist",
    "lint": "eslint --no-error-on-unmatched-pattern –-ext .ts src",
    "lint:fix": "eslint --no-error-on-unmatched-pattern –-ext .ts src --fix",
    "debug": "node --inspect=0.0.0.0:5801 --require ts-node/register ./src/server.ts",
    "start": "node dist/src/server.js",
    "prod": "npm run build && npm run start",
    "tsc": "tsc",
    "format": "prettier --no-error-on-unmatched-pattern --write \"src//*.ts\" \"test//*.ts\""
  },
  "keywords": [],
  "author": "Dmytro Petrenko",
  "license": "ISC",
  "dependencies": {
    "axios": "0.24.0",
    "cookie-parser": "1.4.5",
    "cors": "2.8.5",
    "dotenv": "10.0.0",
    "express": "4.17.1",
    "lodash": "4.17.21",
    "moment": "2.29.2",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "typedi": "0.10.0",
    "winston": "3.3.3"
  },
  "devDependencies": {
    "@types/cookie-parser": "1.4.2",
    "@types/cors": "2.8.12",
    "@types/express": "4.17.9",
    "@types/lodash": "4.14.172",
    "@types/node": "14.14.9",
    "@types/uuid": "8.3.1",
    "@types/winston": "2.4.4",
    "@typescript-eslint/eslint-plugin": "5.4.0",
    "@typescript-eslint/parser": "5.4.0",
    "cross-env": "7.0.3",
    "eslint": "7.28.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "prettier": "2.5.1",
    "ts-node": "9.0.0",
    "typescript": "4.1.2"
  }
}
